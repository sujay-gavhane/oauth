<div class="row">
  <div class="col-md-12">
    <div class="portlet box green">
      <div class="portlet-title">
        <div class="caption">
          <h4>View Repository</h4>
        </div>
      </div>
        <div class="portlet-body form" >
          <table  class="table table-striped table-bordered table-hover no-footer" role="grid">
            <tbody>
              <tr role="row" class="row">
                <th class="col-md-3">Name</th>
                <td class="col-md-9"><%= params['id'] %></td>
              </tr>
              <tr role="row" class="row" >
                <th class="col-md-3">Description</th>
                <td class="col-md-9"><%= params['description'] %></td>
              </tr>
            </tbody>
          </table>
          <%= form_tag(repo_commits_repository_path(id: params['id']), method: :get, remote: true) do %>
            <div class="form-group">
              <%= render 'search' %>
            </div>
          <% end %>
          <div class="commits-listing">
            <%#= render 'listing' %>
          </div>
        </div>
    </div>
  </div>
</div>