<div class="row">
  <div class="col-md-12">
    <div class="portlet box green">
      <div class="portlet-title">
        <div class="caption">
          <i class="fa fa-list-ul badge-warning"></i>View Repository
        </div>
      </div>
        <div class="portlet-body form" id='course-listing'>
          <table  class="table table-striped table-bordered table-hover no-footer" role="grid" id="course_listing_table">
            <tbody>
              <tr role="row" class='columnWrap'>
                <th class="col-md-3">Name</th>
                <td class="col-md-3"><%= params['id'] %></td>
              </tr>
                <tr role="row" class="odd" data-no-turbolink >
                  <th class="col-md-3">Description</th>
                  <td class="col-md-3"><%= params['description'] %></td>
                </tr>
            </tbody>
          </table>
          <% if false %>
            <div class="portlet-body">
              <div class="row">
                <div class="col-md-12">
                  <h4>No commits present!</h4>
                </div>
              </div>
            </div>
          <% else %>
            <h4>Commits</h4>
            <div class="row">
              <div class="col-md-9">
                Commit Message
              </div>
              <div class="col-md-3">
                Date
              </div>
              <% @response.each do |commit| %>
                <div class="col-md-9">
                  <%= commit['commit']['message']%>
                </div>
                <div class="col-md-3">
                  <%= commit['commit']['committer']['date']%>
                </div>
              <% end %>
            </div>
          <%end%>
        </div>
    </div>
  </div>
</div>